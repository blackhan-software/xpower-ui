{
    "files.exclude": {
        "**/*.css.map": {
            "when": "$(basename).map"
        },
        "**/*.css": {
            "when": "$(basename).scss"
        },
        "**/*.js.map": {
            "when": "$(basename).map"
        },
        "**/*.js": {
            "when": "$(basename).ts"
        },
    },
    "git.ignoreLimitWarning": true
}
