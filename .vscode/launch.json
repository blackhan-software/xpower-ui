{
    "configurations": [
        {
            "console": "integratedTerminal",
            "name": "Dev [Main]",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev@mainnet"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
        },
        {
            "console": "integratedTerminal",
            "name": "Dev [Test]",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "dev@testnet"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
        },
        {
            "console": "integratedTerminal",
            "name": "TSC Watch",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script",
                "watch"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
        },
        {
            "name": "Attach",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
    ],
}