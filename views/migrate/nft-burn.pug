form.burn-nft
  label.form-label
    | Burning of Empty NFTs [v3a]
  div.btn-group.burn-empty-nft.thor(role="group")
    button.burn-empty-nft.thor.btn.btn-outline-warning(
      type="button" data-source="v3a" data-target="v3b"
    )
      | Burn Empty PARA NFTs
    button.btn.btn-outline-warning.info(
      type="button" data-bs-toggle="tooltip" data-bs-placement="top"
      title="Execute burn of empty PARA NFTs (i.e. amount=0)"
    )
      include ../../public/images/pug/info-circle-fill.svg.pug
  div.btn-group.burn-empty-nft.loki(role="group")
    button.burn-empty-nft.loki.btn.btn-outline-warning(
      type="button" data-source="v3a" data-target="v3b"
    )
      | Burn Empty AQCH NFTs
    button.btn.btn-outline-warning.info(
      type="button" data-bs-toggle="tooltip" data-bs-placement="top"
      title="Execute burn of empty AQCH NFTs (i.e. amount=0)"
    )
      include ../../public/images/pug/info-circle-fill.svg.pug
  div.btn-group.burn-empty-nft.odin(role="group")
    button.burn-empty-nft.odin.btn.btn-outline-warning(
      type="button" data-source="v3a" data-target="v3b"
    )
      | Burn Empty QRSH NFTs
    button.btn.btn-outline-warning.info(
      type="button" data-bs-toggle="tooltip" data-bs-placement="top"
      title="Execute burn of empty QRSH NFTs (i.e. amount=0)"
    )
      include ../../public/images/pug/info-circle-fill.svg.pug
